<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="listenerSection" 
             type="MP.WindowsServices.Common.ConfigurationHelper.AppConfiguration, MP.WindowsServices.Common"/>
  </configSections>
  <listenerSection>
    <fileNameTemplate template="[a-zA-Z0-1]+_[0-9]{3}.[a-zA-Z]{3,4}" />
    <fileFilters filters="jpg, jpeg, png, tiff" />
    <observableFolders>
      <folder path="D:\ObservableFolder" />
    </observableFolders>
  </listenerSection>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
  </startup>
</configuration>